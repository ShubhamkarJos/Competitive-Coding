        "class Solution:\r\n",
        "    def findMedianSortedArrays(self, nums1: List[int], nums2: List[int]) -> float:\r\n",
        "        \r\n",
        "        for i in nums2:\r\n",
        "            nums1.append(i)\r\n",
        "        import statistics\r\n",
        "        return statistics.median(nums1)"
